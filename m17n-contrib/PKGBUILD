pkgname=m17n-contrib
pkgver=1.1.12
pkgrel=1
pkgdesc="Contributions (input methods) for m17n"
url="http://www.nongnu.org/m17n"
arch=('any')
license=('GPL')
depends=('gawk' 'm17n-db' 'm17n-lib')
source=(http://pkgs.fedoraproject.org/repo/pkgs/m17n-contrib/m17n-contrib-1.1.12.tar.gz/fb7c35194e8940cf3ca381f5b111434c/$pkgname-$pkgver.tar.gz)
build() {
  cd $srcdir/$pkgname-$pkgver
  ./configure --prefix=/usr
  make 
  make DESTDIR=$pkgdir install
}
md5sums=('fb7c35194e8940cf3ca381f5b111434c')
